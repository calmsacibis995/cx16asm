    .PAGE 'CX16ASM TEST PROGRAM'CHAROUT = $FFD2    *=$0801;; BASIC STUB;BASIC   .BYTE $0B, $08, $01, $00, $9E, $32, $30, $36, $31, $00, $00, $00        LDA #9  ;OPEN CHARSET FOR CHANGE        JSR CHAROUT        LDA #14 ;LOWER CASE CHARSET        JSR CHAROUT        LDA #8  ;CLOSE CHARSET        JSR CHAROUT    .END    